<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.1</Version>
    <Authors>ChristianW</Authors>
    <Company>ChristianW</Company>
    <Description>Abstractions to quickly create exports from sql queries.</Description>
    <Copyright>ChristianW</Copyright>
    <LangVersion>latest</LangVersion>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\Toolkit.Module.DataExport.SQLExecutor.xml</DocumentationFile>
    <OutputPath>bin\$(Configuration)\$(TargetFrameworkVersion)\</OutputPath>
    <PackageProjectUrl>https://github.com/chhjel/Releases/releases/tag/nuget-Toolkit</PackageProjectUrl>
    <AssemblyName>Toolkit.Module.DataExport.SQLExecutor</AssemblyName>
  </PropertyGroup>

  <PropertyGroup>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <GenerateBindingRedirectsOutputType>true</GenerateBindingRedirectsOutputType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Data.SqlClient" Version="4.4.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Toolkit.Core\Toolkit.Core.csproj" />
    <ProjectReference Include="..\Toolkit.Module.DataExport\Toolkit.Module.DataExport.csproj" />
  </ItemGroup>

</Project>
