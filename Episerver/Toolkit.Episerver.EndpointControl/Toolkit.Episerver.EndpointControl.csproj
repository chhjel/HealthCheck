<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.1;netcoreapp3.1;net461;net472;net6.0</TargetFrameworks>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.1</Version>
    <Authors>ChristianW</Authors>
    <Company>ChristianW</Company>
    <Description>Episerver related implementations for the EndpointControl module.</Description>
    <Copyright>ChristianW</Copyright>
    <LangVersion>latest</LangVersion>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\Toolkit.Episerver.EndpointControl.xml</DocumentationFile>
    <OutputPath>bin\$(Configuration)\$(TargetFrameworkVersion)\</OutputPath>
    <PackageProjectUrl>https://github.com/chhjel/Releases/releases/tag/nuget-Toolkit</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Toolkit.Core\Toolkit.Core.csproj" />
    <ProjectReference Include="..\..\Modules\Toolkit.Module.EndpointControl\Toolkit.Module.EndpointControl.csproj" />
    <ProjectReference Include="..\Toolkit.Episerver\Toolkit.Episerver.csproj" />
  </ItemGroup>

</Project>
