<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.1;netcoreapp3.1;net461;net472;net6.0</TargetFrameworks>
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Version>1.0.1</Version>
		<Authors>ChristianW</Authors>
		<Company>ChristianW</Company>
		<Description>Episerver related implementations for the IPWhitelist module.</Description>
		<Copyright>ChristianW</Copyright>
		<LangVersion>latest</LangVersion>
		<DocumentationFile>bin\$(Configuration)\$(TargetFramework)\QoDL.Toolkit.Episerver.IPWhitelist.xml</DocumentationFile>
		<OutputPath>bin\$(Configuration)\$(TargetFrameworkVersion)\</OutputPath>
		<PackageProjectUrl>https://github.com/chhjel/Releases/releases/tag/nuget-Toolkit</PackageProjectUrl>
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\..\Modules\Toolkit.Module.IPWhitelist\QoDL.Toolkit.Module.IPWhitelist.csproj" />
		<ProjectReference Include="..\..\Toolkit.Core\QoDL.Toolkit.Core.csproj" />
		<ProjectReference Include="..\Toolkit.Episerver\QoDL.Toolkit.Episerver.csproj" />
	</ItemGroup>

</Project>
